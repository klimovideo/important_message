# Telegram Bot Token (получить у @BotFather)
TELEGRAM_TOKEN=your_bot_token_here

# GigaChat API credentials (получить на https://developers.sber.ru/)
CLIENT_ID=your_gigachat_client_id
SECRET=your_gigachat_secret

# Userbot credentials (для скрытого мониторинга через фейковый аккаунт)
# Получить на https://my.telegram.org/apps
TELEGRAM_API_ID=your_api_id
TELEGRAM_API_HASH=your_api_hash
TELEGRAM_PHONE=+7xxxxxxxxxx

# Настройки
LOG_LEVEL=INFO
USERBOT_ENABLED=true 